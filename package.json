{
  "name": "@movieticket/reserve-api-abstract-client",
  "version": "1.0.1",
  "description": "Movieticket Reserve API Abstract Client Library for Javascript",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "doc",
    "example",
    "lib"
  ],
  "directories": {
    "doc": "./doc",
    "lib": "./lib",
    "example": "./example",
    "test": "./test"
  },
  "scripts": {
    "build": "tsc",
    "check": "npm run tslint",
    "clean": "rimraf ./lib \"npm-debug.log*\" ./doc ./.nyc_output",
    "doc": "jsdoc -c jsdoc.json",
    "mocha": "mocha",
    "nyc": "nyc mocha \"src/**/*.spec.ts\"",
    "coverage": "npm run nyc && nyc report --reporter=text-lcov | coveralls",
    "snyk": "snyk wizard",
    "test": "npm run check && npm run coverage",
    "tslint": "tslint --project tsconfig.json -c tslint.json --exclude \"**/*.spec.ts\" src/**/*.ts",
    "preversion": "npm run clean && npm run build && npm test",
    "version": "git add -A",
    "postversion": "git push origin --tags",
    "prepublishOnly": "npm run clean && npm run build && npm test && npm run doc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/movieticket/reserve-api-abstract-client.git"
  },
  "keywords": [
    "movieticket"
  ],
  "author": {
    "name": "Motionpicture co.,ltd."
  },
  "contributors": [
    {
      "email": "yamazaki@motionpicture.jp",
      "name": "Tetsu Yamazaki"
    }
  ],
  "license": "ISC",
  "devDependencies": {
    "@types/debug": "0.0.30",
    "@types/fetch-mock": "^6.0.4",
    "@types/form-data": "^2.2.1",
    "@types/http-status": "^0.2.30",
    "@types/isomorphic-fetch": "0.0.34",
    "@types/mocha": "^5.2.5",
    "@types/nock": "^9.1.3",
    "@types/open": "0.0.29",
    "@types/power-assert": "^1.5.0",
    "@types/qs": "^6.5.1",
    "@types/sinon": "^4.3.3",
    "coveralls": "^3.0.1",
    "fetch-mock": "^6.5.2",
    "http-server": "^0.10.0",
    "ink-docstrap": "^1.3.2",
    "jsdoc": "^3.5.5",
    "mocha": "^5.2.0",
    "nock": "^9.3.2",
    "nyc": "^12.0.2",
    "open": "0.0.5",
    "power-assert": "^1.6.1",
    "rimraf": "^2.6.2",
    "sinon": "^5.1.1",
    "snyk": "^1.103.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-microsoft-contrib": "^5.2.1",
    "typescript": "^3.1.2"
  },
  "dependencies": {
    "@motionpicture/mvtk-reserve-service": "^1.2.1",
    "debug": "^3.2.5",
    "http-status": "^1.1.2",
    "isomorphic-fetch": "^2.2.1",
    "qs": "^6.5.2"
  }
}
